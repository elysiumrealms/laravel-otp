{
    "name": "elysiumrealms/laravel-otp",
    "description": "A package to generate and validate OTP",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "otp", "one-time-password", "authentication"],
    "homepage": "https://github.com/elysiumrealms/laravel-otp",
    "version": "1.1.0",
    "authors": [
        {
            "name": "deflinhec",
            "email": "deflinhec@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Elysiumrealms\\Otp\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.1.0",
        "laravel/framework": "~5.5|~6.0|~7.0|~8.0|~9.0|~10.0",
        "laravel-notification-channels/twilio": "^3.4",
        "laravel-notification-channels/telegram": "^2.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Elysiumrealms\\Otp\\ServiceProvider"
            ],
            "aliases": {
                "Otp": "Elysiumrealms\\Otp\\Facades\\Otp"
            }
        }
    }
}
